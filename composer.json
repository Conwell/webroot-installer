{
    "name": "conwell/webroot-installer",
    "type": "composer-installer",
    "license": "BSD-3-Clause",
    "description": "A composer installer for libraries that live in an application webroot.",
    "authors": [
        {
            "name": "Steve Buzonas",
            "email": "steve@fancyguy.com"
        },
        {
            "name": "Alex Conwell"     
        }
    ],
    "autoload": {
        "psr-0": { "FancyGuy\\Composer\\": "src/" }
    },
    "extra": {
        "class": "FancyGuy\\Composer\\WebrootInstaller"
    },
    "require-dev": {
        "composer/composer": "1.4.*@dev",
        "phpunit/phpunit": "6.2.*"
    }
}
